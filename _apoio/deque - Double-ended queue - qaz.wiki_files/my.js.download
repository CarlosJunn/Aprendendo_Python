function copy_right() {
    console.log('Copy Right ...', location.hostname);
    var cr_h1_arr = document.querySelectorAll("h1");
    console.log(cr_h1_arr.length);
    if (cr_h1_arr.length > 0)
    {
        console.log("document.location.href", document.location.href);
        var cr_h1_innerHTML = cr_h1_arr[0].innerHTML;
        console.log("cr_h1_innerHTML", cr_h1_innerHTML);
        var cr_h1_pos = cr_h1_innerHTML.search(/\s*-\s*<span class="notranslate">/gm);
        console.log("cr_h1_pos", cr_h1_pos);
        var cr_h1_title = cr_h1_innerHTML.substr(0,cr_h1_pos);
        console.log("cr_h1_title", cr_h1_title);

        var bodyElement = document.body; 
        var selection = getSelection(); 
        var href = document.location.href; 
        //var copyright = 'Источник: <a href="'+ href +'">' + href + '</a>'; 
        var copyright = "<br /><a href='"+document.location.href+"'>"+cr_h1_title+"</a><br />";; 
        var text = selection + copyright;
        var divElement = document.createElement('div'); 
        divElement.style.position = 'absolute'; 
        divElement.style.left = '-99999px'; 
        text1 = document.createTextNode(text); //создал текстовый узел
        divElement.appendChild(text1); //и добавил его
        bodyElement.appendChild(divElement); 
        selection.selectAllChildren(divElement); 
        setTimeout(function(){
          bodyElement.removeChild(divElement); 
        }, 0);
    }
    
}

